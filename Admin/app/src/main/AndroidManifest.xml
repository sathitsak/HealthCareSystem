<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.admin" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/admin"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Choosemenu"
            android:label="drugmenu"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Drugmenu"
            android:label="drugmenu"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Adddrug"
            android:label="adddrug"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Showdrug"
            android:label="showdrug"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Deletedrug"
            android:label="deletedrug"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Infodrug"
            android:label="infordrug"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Usermenu"
            android:label="usermenu"
            android:screenOrientation="portrait"
            />
        <activity
        android:name=".Deleteuser"
        android:label="deleteuser"
        android:screenOrientation="portrait"
        />
    </application>

</manifest>
